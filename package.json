		"@types/node": "^14.18.63",
		"@typescript-eslint/eslint-plugin": "^8.27.0",
		"@typescript-eslint/parser": "^8.27.0",
		"commitizen": "^4.3.1",
		"commitlint-config-gitmoji": "^2.3.1",
		"cross-env": "^6.0.3",
		"cz-conventional-emoji": "^1.0.2",
		"cz-emoji-chinese": "^0.3.1",
		"eslint": "9.22.0",
		"globals": "^16.0.0",
		"husky": "^9.1.7",
		"jest": "^29.7.0",
